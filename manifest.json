{
  "manifest_version": 3,
  "name": "TimeUp",
  "version": "1.0.0",
  "description": "Extension to improve the time management",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icons": {
      "16": "icon.png",
      "24": "icon.png",
      "32": "icon.png"
    },
    "default_title": "TimeUp",
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "alarms", "notifications", "tabs"],
  "background": {
    "service_worker": "background.js"
  }
}